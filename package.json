{
  "name": "svelte-mdc",
  "description": "Material Design components for Svelte",
  "version": "0.2.0",
  "author": "Kiho Chang",
  "license": "MIT",
  "module": "dist/svelte-mdc.mjs",
  "svelte": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kiho/svelte-mdc.git"
  },
  "scripts": {
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "autolib": "rollup -c rollup.lib.config.js -w",
    "dev": "run-p start:dev autobuild",
    "start": "sirv public",
    "start:dev": "sirv public --dev",
    "release": "standard-version"
  },
  "peerDependencies": {
    "material-components-web": ">= 2.3.0",
    "svelte": ">= 3.5.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "material-components-web": "^2.3.0",
    "eslint": "^6.0.1",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.13.0",
    "rollup-plugin-cleanup": "^3.1.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-multi-entry": "^2.1.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-svelte": "^5.0.0",
    "rollup-plugin-terser": "^5.0.0",
    "sirv-cli": "^0.4.3",
    "standard": "^10.0.3",
    "standard-version": "^4.2.0",
    "svelte": "^3.6.7"
  }
}
